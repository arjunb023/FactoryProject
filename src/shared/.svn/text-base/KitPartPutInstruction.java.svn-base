package shared;

import java.util.ArrayList;

import shared.enums.PartType;

public class KitPartPutInstruction
{
	ArrayList<PartType>	partsToPut	= new ArrayList<PartType>();
	int					destKitStandLoc;
	
	public KitPartPutInstruction(ArrayList<PartType> parts, int kitNum)
	{
		this.partsToPut			= parts;
		this.destKitStandLoc	= kitNum;
	}
	
	public ArrayList<PartType> getPartsToPut()
	{
		return this.partsToPut;
	}
	
	public int getDestKitStandLoc()
	{
		return this.destKitStandLoc;
	}
}
