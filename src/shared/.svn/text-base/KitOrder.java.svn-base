package shared;

import java.util.ArrayList;

import shared.enums.PartType;

public class KitOrder {

	public ArrayList <PartType> kitOrder; //list of parts in a kit

	public KitOrder()
	{
		kitOrder	= new ArrayList<PartType>();
	}
	
	public KitOrder(ArrayList<PartType> kitSpec)
	{
		kitOrder	= kitSpec;
	}
	
	public ArrayList <PartType> getKitOrder() 
	{
		return kitOrder;
	}
	//Set by user
	public void setKitOrder(ArrayList<PartType> order)
	{
		kitOrder	= order;
	}
}
